<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="720.0" prefWidth="1080.0" stylesheets="@menu.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menu.StartController">
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-alignment: center;" BorderPane.alignment="CENTER">
         <children>
            <Label text="NOME CASINO" />
            <Label text="Continua la tua sessione accedendo, oppure cominciane una vuova creando una nuova utenza" />
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-alignment: center;">
               <children>
                  <Button fx:id="btLogIn" mnemonicParsing="false" onAction="#gologInPage" text="Log-in" />
                  <Button fx:id="btSignIn" mnemonicParsing="false" onAction="#gosignInPage" text="Sign-in" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
